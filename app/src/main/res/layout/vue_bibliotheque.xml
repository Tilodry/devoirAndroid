<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".vue.VueBibliotheque">

    <ListView
        android:id="@+id/vueBibliothequeListeLivre"
        android:layout_width="422dp"
        android:layout_height="524dp"
        app:layout_constraintBottom_toTopOf="@+id/vueBibliothequeListeLivreActionAjouterLivre"
        tools:layout_editor_absoluteX="-11dp" />

    <Button
        android:id="@+id/vueBibliothequeListeLivreActionAjouterLivre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ajouter un livre"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>